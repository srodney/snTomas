	Adsurl = {http://adsabs.harvard.edu/abs/2003Natur.424..651H},
	Author = {{Hamuy}, M. and {Phillips}, M.~M. and {Suntzeff}, N.~B. and {Maza}, J. and {Gonz{\'a}lez}, L.~E. and {Roth}, M. and {Krisciunas}, K. and {Morrell}, N. and {Green}, E.~M. and {Persson}, S.~E. and {McCarthy}, P.~J.},
	Date-Added = {2009-06-23 18:16:12 -0700},
