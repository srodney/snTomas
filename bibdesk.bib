	Doi = {10.1086/491664},
	Eprint = {arXiv:astro-ph/0508180},
	Journal = {\apj},
	Keywords = {Cosmology: Distance Scale, Stars: Supernovae: General},
	Month = nov,
	Pages = {210-226},
	Read = {Yes},
	Title = {{Chemistry and Star Formation in the Host Galaxies of Type Ia Supernovae}},
	Url = {http://adsabs.harvard.edu/abs/2005ApJ...634..210G},
	Volume = 634,
	Year = 2005,
	Bdsk-File-1 = {},
