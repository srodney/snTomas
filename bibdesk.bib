	Date-Modified = {2014-09-07 19:03:03 +0000},
	Doi = {10.1088/0004-637X/723/1/329},
	Eprint = {1006.5751},
	Journal = {\apj},
	Keywords = {gamma rays: diffuse background, gamma rays: stars, nuclear reactions, nucleosynthesis, abundances, stars: statistics, supernovae: general},
	Month = nov,
	Pages = {329-341},
	Primaryclass = {astro-ph.CO},
	Title = {{Revealing Type Ia Supernova Physics with Cosmic Rates and Nuclear Gamma Rays}},
