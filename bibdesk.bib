	Eprint = {0708.1787},
	Journal = {\mnras},
	Keywords = {dense matter , stars: evolution , stars: neutron , supernovae: individual: SN2006gy},
	Month = jul,
	Pages = {1193-1198},
	Read = {Yes},
	Title = {{Supernova SN2006gy as a first ever Quark Nova?}},
	Url = {http://adsabs.harvard.edu/abs/2008MNRAS.387.1193L},
