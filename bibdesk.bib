	Journal = {\apj},
	Keywords = {Galaxies: Clusters: General; Stars: Supernovae: General; supernovae: individual (SN 1998eu); supernovae: individual (SN 1998fc); supernovae: individual (SN 1999cg); supernovae: individual (SN 1999ch); supernovae: individual (SN 1999ci); supernovae: individual (SN 1999ct)},
	Pages = {1165--1175},
	Read = {Yes},
