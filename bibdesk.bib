	Doi = {10.1051/0004-6361/201014115},
	Eprint = {1003.2491},
	Journal = {\aap},
	Keywords = {supernovae: general, binaries: close, white dwarfs, galaxies: elliptical, and lenticular, cD},
	Month = jun,
	Pages = {A89+},
	Primaryclass = {astro-ph.SR},
	Title = {{The delay-time distribution of Type Ia supernovae: a comparison between theory and observation}},
