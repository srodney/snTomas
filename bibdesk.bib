	Arxivurl = {http://arXiv.org/abs/astro-ph/9708120},
	Author = {{Kolatt}, T.~S. and {Bartelmann}, M.},
	Date-Added = {2015-04-05 05:56:51 +0000},
	Date-Modified = {2015-04-05 05:56:51 +0000},
	Doi = {10.1046/j.1365-8711.1998.01466.x},
	Eprint = {astro-ph/9708120},
	Journal = {\mnras},
	Keywords = {METHODS: MISCELLANEOUS, GALAXIES: CLUSTERS: GENERAL, DARK MATTER, GRAVITATIONAL LENSING},
	Month = may,
	Pages = {763-772},
	Title = {{Gravitational lensing of type IA supernovae by galaxy clusters}},
