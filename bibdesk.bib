	Keywords = {Galaxies: Distances and Redshifts; galaxies: individual (M88); supernovae: general; supernovae: individual (SN 1998bu)},
	Pages = {658--674},
	Read = {Yes},
	Title = {Uniformity of (V-Near-Infrared) Color Evolution of Type Ia Supernovae and Implications for Host Galaxy Extinction Determination},
	Ty = {JOUR},
