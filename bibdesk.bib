	Doi = {10.1086/506588},
	Eprint = {arXiv:astro-ph/0606449},
	Journal = {\apj},
	Keywords = {Radiative Transfer, Stars: Supernovae: General},
	Month = oct,
	Pages = {939-953},
	Read = {Yes},
	Title = {{Secondary Maximum in the Near-Infrared Light Curves of Type Ia Supernovae}},
	Url = {http://adsabs.harvard.edu/abs/2006ApJ...649..939K},
