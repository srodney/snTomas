	Doi = {10.1088/0004-637X/704/1/629},
	Eprint = {0908.0536},
	Journal = {\apj},
	Keywords = {distance scale, supernovae: general},
	Month = oct,
	Pages = {629-651},
	Read = {Yes},
	Title = {{Type Ia Supernova Light-Curve Inference: Hierarchical Bayesian Analysis in the Near-Infrared}},
