	Month = sep,
	Pages = {385-432},
	Read = {Yes},
	Title = {{Dark Energy and the Accelerating Universe}},
	Url = {http://adsabs.harvard.edu/abs/2008ARA%26A..46..385F},
	Volume = 46,
