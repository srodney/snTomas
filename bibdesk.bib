	Arxivurl = {http://arXiv.org/abs/astro-ph/9704090},
	Author = {{Franx}, M. and {Illingworth}, G.~D. and {Kelson}, D.~D. and {van Dokkum}, P.~G. and {Tran}, K.-V.},
	Date-Added = {2015-04-05 05:52:45 +0000},
	Date-Modified = {2015-04-05 05:52:46 +0000},
	Doi = {10.1086/310844},
