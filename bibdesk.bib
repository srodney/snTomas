	Keywords = {Hydrodynamics; Instabilities; Methods: Numerical; Stars: Supernovae: General; Turbulence},
	Pages = {1132--1139},
	Read = {Yes},
	Title = {A Three-Dimensional Deflagration Model for Type Ia Supernovae Compared with Observations},
