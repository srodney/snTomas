	Eprint = {1403.4797},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - High Energy Astrophysical Phenomena, Astrophysics - Solar and Stellar Astrophysics},
	Month = mar,
	Primaryclass = {astro-ph.HE},
	Title = {{The influence of mass-transfer variability on the growth of white dwarfs, and the implications for supernova type Ia rates}},
