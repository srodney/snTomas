	Doi = {10.1086/424036},
	Eprint = {arXiv:astro-ph/0405163},
	Journal = {\apjl},
	Keywords = {Hydrodynamics, Instabilities, Stars: Interiors, Stars: Supernovae: General, Stars: White Dwarfs},
	Month = sep,
	Pages = {L37-L40},
	Read = {Yes},
	Title = {{Type Ia Supernova Explosion: Gravitationally Confined Detonation}},
