	Doi = {10.1051/0004-6361/200911869},
	Eprint = {0905.0272},
	Journal = {\aap},
	Keywords = {Galaxy: evolution, stars: supernovae: general, Galaxy: solar neighbourhood},
	Month = jul,
	Pages = {531-538},
	Title = {{The effect of different type Ia supernova progenitors on Galactic chemical evolution}},
	Url = {http://adsabs.harvard.edu/abs/2009A%26A...501..531M},
