	Doi = {10.1088/0004-637X/709/2/1420},
	Eprint = {0909.2692},
	Journal = {\apj},
	Keywords = {cosmology: theory, distance scale, large-scale structure of universe, supernovae: general},
	Month = feb,
	Pages = {1420-1428},
	Title = {{Cosmology with Photometric Surveys of Type Ia Supernovae}},
	Url = {http://adsabs.harvard.edu/abs/2010ApJ...709.1420G},
