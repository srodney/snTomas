	Adsurl = {http://adsabs.harvard.edu/abs/2007ApJ...655..144M},
	Author = {{Mei}, S. and {Blakeslee}, J.~P. and {C{\^o}t{\'e}}, P. and {Tonry}, J.~L. and {West}, M.~J. and {Ferrarese}, L. and {Jord{\'a}n}, A. and {Peng}, E.~W. and {Anthony}, A. and {Merritt}, D.},
