	Eprint = {0708.2745},
	Journal = {Astroparticle Physics},
	Month = dec,
	Pages = {448-455},
	Title = {{Measuring type Ia supernova distances and redshifts from their multi-band light curves}},
	Url = {http://adsabs.harvard.edu/abs/2007APh....28..448K},
