	Date-Modified = {2009-10-22 09:55:32 -1000},
	Doi = {10.1086/424032},
	Eprint = {arXiv:astro-ph/0407170},
	Journal = {\apj},
	Keywords = {Galaxies: Evolution, Galaxies: Starburst, Radio Continuum: Galaxies},
	Month = nov,
	Pages = {209-221},
	Title = {{On the Evolution of Star-forming Galaxies}},
