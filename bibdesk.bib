	Doi = {10.1093/mnras/stu2225},
	Eprint = {1409.0544},
	Journal = {\mnras},
	Keywords = {gravitation, nuclear reactions, nucleosynthesis, abundances, binaries: close, stars: neutron, supernovae: general},
	Month = jan,
	Pages = {1115-1120},
	Primaryclass = {astro-ph.HE},
