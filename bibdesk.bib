	Date-Modified = {2009-08-21 19:43:32 -1000},
	Journal = {\pasp},
	Keywords = {Stars: Supernovae: General; Stars: Supernovae: Individual: Alphanumeric: SN 1991T; supernovae: individual (SN 2000cx)},
	Pages = {1178--1204},
	Read = {Yes},
	Title = {The Unique Type Ia Supernova 2000cx in NGC 524},
