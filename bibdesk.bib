	Adsurl = {http://adsabs.harvard.edu/abs/2009A%26A...504..727L},
	Archiveprefix = {arXiv},
	Author = {{Le Borgne}, D. and {Elbaz}, D. and {Ocvirk}, P. and {Pichon}, C.},
	Date-Added = {2009-10-14 22:00:21 -1000},
	Date-Modified = {2009-10-14 22:00:25 -1000},
