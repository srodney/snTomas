	Author = {{Brown}, P.~J. and {Dessart}, L. and {Holland}, S.~T. and {Immler}, S. and {Landsman}, W. and {Blondin}, S. and {Blustin}, A.~J. and {Breeveld}, A. and {Dewangan}, G.~C. and {Gehrels}, N. and {Hutchins}, R.~B. and {Kirshner}, R.~P. and {Mason}, K.~O. and {Mazzali}, P.~A. and {Milne}, P. and {Modjaz}, M. and {Roming}, P.~W.~A.},
