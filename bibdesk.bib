	Author = {{Goobar}, A. and {Paech}, K. and {Stanishev}, V. and {Amanullah}, R. and {Dahl{\'e}n}, T. and {J{\"o}nsson}, J. and {Kneib}, J.~P. and {Lidman}, C. and {Limousin}, M. and {M{\"o}rtsell}, E. and {Nobili}, S. and {Richard}, J. and {Riehm}, T. and {von Strauss}, M.},
