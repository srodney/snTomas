	Doi = {10.1088/0004-637X/713/1/32},
	Eprint = {1002.2494},
	Journal = {\apj},
	Keywords = {supernovae: general},
	Month = apr,
	Pages = {32-40},
	Title = {{Empirical Delay-time Distributions of Type Ia Supernovae from the Extended Goods/Hubble Space Telescope Supernova Survey}},
