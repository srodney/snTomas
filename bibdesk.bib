	Month = apr,
	Pages = {373-+},
	Read = {Yes},
	Series = {ASP Conference Series},
	Title = {{Preliminary Results from Detector-Based Throughput Calibration of the CTIO Mosaic Imager and Blanco Telescope Using a Tunable Laser}},
