	Doi = {10.1086/309975},
	Eprint = {arXiv:astro-ph/9601050},
	Journal = {\apjl},
	Keywords = {GALAXIES: EVOLUTION, COSMOLOGY: OBSERVATIONS},
	Month = mar,
	Pages = {L1+},
	Title = {{The Canada-France Redshift Survey: The Luminosity Density and Star Formation History of the Universe to Z approximately 1}},
