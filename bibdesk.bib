	Archiveprefix = {arXiv},
	Author = {{Ruiter}, A.~J. and {Sim}, S.~A. and {Pakmor}, R. and {Kromer}, M. and {Seitenzahl}, I.~R. and {Belczynski}, K. and {Fink}, M. and {Herzog}, M. and {Hillebrandt}, W. and {R{\"o}pke}, F.~K. and {Taubenberger}, S.},
	Date-Added = {2013-02-22 16:11:50 +0000},
	Date-Modified = {2013-02-22 16:11:50 +0000},
	Doi = {10.1093/mnras/sts423},
	Eprint = {1209.0645},
