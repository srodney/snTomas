	Date-Added = {2009-06-25 10:05:13 -1000},
	Date-Modified = {2009-06-25 10:05:17 -1000},
	Eprint = {arXiv:astro-ph/9707119},
	Journal = {\mnras},
	Keywords = {STARS: GENERAL, SUPERNOVAE: GENERAL, INFRARED: STARS},
	Month = oct,
	Pages = {663-679},
	Read = {Yes},
	Title = {{Infrared and optical spectroscopy of Type IA supernovae in the nebular phase}},
	Url = {http://adsabs.harvard.edu/abs/1997MNRAS.290..663B},
	Volume = 290,
	Year = 1997,
	Bdsk-Url-1 = {http://adsabs.harvard.edu/abs/1997MNRAS.290..663B}}

@article{Branch:2003,
	Abstract = {
We present 29 optical spectra of the normal Type Ia SN 1998aq, ranging
