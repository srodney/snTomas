	Doi = {10.1038/nature02474},
	Eprint = {arXiv:astro-ph/0403293},
	Journal = {\nat},
	Month = apr,
	Pages = {625-627},
	Title = {{The star-formation history of the Universe from the stellar populations of nearby galaxies}},
	Url = {http://adsabs.harvard.edu/abs/2004Natur.428..625H},
	Volume = 428,
	Year = 2004,
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/nature02474},
