	Author = {{Richard}, J. and {Pei}, L. and {Limousin}, M. and {Jullo}, E. and {Kneib}, J.~P.},
	Date-Added = {2014-04-09 17:03:06 +0000},
	Date-Modified = {2014-04-09 17:03:06 +0000},
	Doi = {10.1051/0004-6361/200811366},
	Eprint = {0901.0427},
