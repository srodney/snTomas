	Adsurl = {http://adsabs.harvard.edu/abs/2008PhRvD..77l3525W},
	Archiveprefix = {arXiv},
	Author = {{Wang}, Y.},
	Date-Added = {2009-11-06 01:42:58 -1000},
	Date-Modified = {2009-11-06 01:44:08 -1000},
	Doi = {10.1103/PhysRevD.77.123525},
	Eprint = {0803.4295},
	Journal = {\prd},
	Month = jun,
	Number = 12,
	Pages = {123525-+},
	Title = {{Figure of merit for dark energy constraints from current observational data}},
	Url = {http://adsabs.harvard.edu/abs/2008PhRvD..77l3525W},
	Volume = 77,
	Year = 2008,
