	Doi = {10.1088/0004-637X/710/2/1310},
	Eprint = {0910.4992},
	Journal = {\apj},
	Keywords = {binaries: close, stars: evolution, stars: dwarf novae, supernovae: general, white dwarfs},
	Month = feb,
	Pages = {1310-1323},
	Title = {{A Comprehensive Progenitor Model for SNe Ia}},
