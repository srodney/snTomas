	Date-Modified = {2009-10-17 14:13:28 -1000},
	Doi = {10.1111/j.1365-2966.2009.14636.x},
	Eprint = {0802.2471},
	Journal = {\mnras},
	Keywords = {binaries: close , stars: evolution , supernovae: general , white dwarfs},
	Month = jun,
	Pages = {2103-2116},
	Title = {{A single-degenerate channel for the progenitors of Type Ia supernovae with different metallicities}},
