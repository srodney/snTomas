	Date-Modified = {2009-06-26 10:13:20 -1000},
	Doi = {10.1086/308376},
	Journal = {\apj},
	Keywords = {DIFFUSION, RADIATIVE TRANSFER, STARS: INTERIORS, STARS: SUPERNOVAE: GENERAL},
	Month = feb,
	Pages = {744-756},
	Read = {Yes},
	Title = {{The Physics of Type IA Supernova Light Curves. I. Analytic Results and Time Dependence}},
	Url = {http://adsabs.harvard.edu/abs/2000ApJ...530..744P},
	Volume = 530,
	Year = 2000,
