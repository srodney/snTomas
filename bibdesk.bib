	Keywords = {galaxies: abundances, galaxies: statistics, galaxies: stellar content, large-scale structure of universe, supernovae: general},
	Month = oct,
	Pages = {687-704},
	Read = {Yes},
	Title = {{The Large-scale Environments of Type Ia Supernovae: Evidence for a Metallicity Bias in the Rate or Luminosity of Prompt Ia Events}},
