	Date-Modified = {2009-11-16 21:52:51 -1000},
	Doi = {10.1086/376588},
	Journal = {\apj},
	Keywords = {Galaxies: Evolution, Galaxies: High-Redshift, Galaxies: Structure, Methods: Data Analysis},
	Month = aug,
	Pages = {640-660},
	Title = {{Cloning Dropouts: Implications for Galaxy Evolution at High Redshift}},
	Url = {http://adsabs.harvard.edu/abs/2003ApJ...593..640B},
	Volume = 593,
