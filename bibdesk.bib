	Journal = {\aap},
	Keywords = {dark matter, galaxies: clusters: general, gravitational lensing},
	Month = sep,
	Pages = {13-22},
	Title = {{Mass-sheet degeneracy: Fundamental limit on the cluster mass reconstruction from statistical (weak) lensing}},
