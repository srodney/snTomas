	Journal = {\apj},
	Keywords = {Cosmology: Distance Scale, Radiative Transfer, Stars: Supernovae: General},
	Month = feb,
	Pages = {661-665},
	Read = {Yes},
	Title = {{On the Origin of the Type Ia Supernova Width-Luminosity Relation}},
