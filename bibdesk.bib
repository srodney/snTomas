	Doi = {10.1088/0004-637X/715/1/323},
	Eprint = {1003.5724},
	Journal = {\apj},
	Keywords = {methods: statistical, supernovae: general},
	Month = may,
	Pages = {323-334},
	Title = {{Fuzzy Supernova Templates. II. Parameter Estimation}},
