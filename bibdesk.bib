	Doi = {10.1111/j.1365-2966.2010.16808.x},
	Eprint = {1002.3056},
	Journal = {\mnras},
	Keywords = {methods: data analysis, supernovae: general, galaxies: star formation},
	Month = apr,
	Pages = {1508-1521},
	Primaryclass = {astro-ph.CO},
	Title = {{Nearby supernova rates from the Lick Observatory Supernova Search - IV. A recovery method for the delay-time distribution}},
	Volume = 412,
	Year = 2011,
