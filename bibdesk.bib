	Date-Modified = {2009-11-21 07:34:58 -1000},
	Doi = {10.1086/591176},
	Eprint = {0806.3457},
	Journal = {\apj},
	Keywords = {Cosmology: Observations, Galaxies: Active, Galaxies: Distances and Redshifts, Galaxies: Evolution, Galaxies: Formation, X-Rays: Galaxies},
	Month = oct,
