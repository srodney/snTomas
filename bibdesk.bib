	Journal = {\nar},
	Month = jun,
	Pages = {122-141},
	Primaryclass = {astro-ph.SR},
	Title = {{Progenitors of type Ia supernovae}},
	Volume = 56,
	Year = 2012,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.newar.2012.04.001}}

