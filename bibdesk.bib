	Doi = {10.1111/j.1365-2966.2004.08326.x},
	Eprint = {arXiv:astro-ph/0403223},
	Journal = {\mnras},
	Keywords = {galaxies: evolution, galaxies: formation, galaxies: high-redshift, galaxies: individual: SBM03\#1, galaxies: starburst, ultraviolet: galaxies},
	Month = dec,
	Pages = {374-384},
	Title = {{The star formation rate of the Universe at z\~{} 6 from the Hubble Ultra-Deep Field}},
	Url = {http://adsabs.harvard.edu/abs/2004MNRAS.355..374B},
